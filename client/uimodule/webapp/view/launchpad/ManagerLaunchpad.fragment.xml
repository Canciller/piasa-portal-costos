<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:f="sap.f"
  xmlns:core="sap.ui.core"
>
  <Page title='{i18n>role.M}'>
    <GenericTile
      class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
      header="{i18n>launchpadManager.tile.manageAssignments.header}"
      subheader="{i18n>launchpadManager.tile.manageAssignments.subheader}"
      width="13rem"
      press=".Manager.onPressManageAssignments"
    >
      <TileContent
      >
        <ImageContent
          class="imageContent"
          src="sap-icon://activity-individual"
        />
      </TileContent>
    </GenericTile>
    <GenericTile
      class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
      header="{i18n>launchpadManager.tile.loadBudget.header}"
      width="13rem"
      press=".Manager.onPressLoadBudget"
    >
      <TileContent
      >
        <ImageContent
          class="imageContent"
          src="sap-icon://expense-report"
        />
      </TileContent>
    </GenericTile>
  </Page>
</core:FragmentDefinition>