<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:f="sap.f"
  xmlns:core="sap.ui.core"
>
  <Page title='{i18n>role.M}'>
    <GenericTile
      class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
      header="{i18n>launchpadManager.tile.assignments.header}"
      subheader="{i18n>launchpadManager.tile.assignments.subheader}"
      width="13rem"
      press=".Manager.onPressManageAssigments"
    >
      <TileContent
      >
        <ImageContent
          class="imageContent"
          src="sap-icon://user-edit"
        />
      </TileContent>
    </GenericTile>
  </Page>
</core:FragmentDefinition>