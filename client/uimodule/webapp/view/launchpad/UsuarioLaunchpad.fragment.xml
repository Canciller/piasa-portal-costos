<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:f="sap.f"
  xmlns:core="sap.ui.core"
>
  <Page title='{i18n>role.U}'>
    <GenericTile
      class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
      header="Reporte 1"
      width="13rem"
      press=".User.onPressReporte1"
    >
      <TileContent
      >
        <ImageContent
          class="imageContent"
          src="sap-icon://activity-individual"
        />
      </TileContent>
    </GenericTile>
    <GenericTile
      class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
      header="Reporte 2"
      width="13rem"
      press=".User.onPressReporte2"
    >
      <TileContent
      >
        <ImageContent
          class="imageContent"
          src="sap-icon://activity-individual"
        />
      </TileContent>
    </GenericTile>
  </Page>
</core:FragmentDefinition>