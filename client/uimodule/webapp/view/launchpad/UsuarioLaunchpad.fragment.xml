<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:f="sap.f"
  xmlns:core="sap.ui.core"
>
  <Page title='{i18n>role.U}'>
    <GenericTile
      class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
      header="{i18n>launchpadUser.tile.reporte1.header}"
      width="13rem"
      press=".User.onPressReporte1"
    >
      <TileContent
      >
        <ImageContent
          class="imageContent"
          src="sap-icon://monitor-payments"
        />
      </TileContent>
    </GenericTile>
    <GenericTile
      class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
      header="{i18n>launchpadUser.tile.reporte2.header}"
      width="13rem"
      press=".User.onPressReporte2"
    >
      <TileContent
      >
        <ImageContent
          class="imageContent"
          src="sap-icon://manager-insight"
        />
      </TileContent>
    </GenericTile>
  </Page>
</core:FragmentDefinition>