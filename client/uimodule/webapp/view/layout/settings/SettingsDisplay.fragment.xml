<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormDisplay354"
			editable="false">
			<f:title>
				<core:Title text="General" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout
					labelSpanXL="3"
					labelSpanL="3"
					labelSpanM="3"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="4"
					emptySpanL="4"
					emptySpanM="4"
					emptySpanS="0"
					columnsXL="1"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer
				>
					<f:formElements>
						<f:FormElement label="Nombre">
							<f:fields>
								<Text text="{user>/name}" id="nameText"  />
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Usuario">
							<f:fields>
								<Text text="{user>/username}" />
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Email">
							<f:fields>
								<Text text="{user>/email}" />
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>